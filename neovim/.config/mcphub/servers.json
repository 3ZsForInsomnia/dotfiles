{
  "mcpServers": {
    "anki-mcp": {
      "args": [
        "--stdio"
      ],
      "command": "anki-mcp-http",
      "env": {
        "ANKI_CONNECT_URL": "http://localhost:8765"
      }
    },
    "atlassian-mcp-server": {
      "type": "http",
      "url": "https://mcp.atlassian.com/v1/sse"
    },
    "basic-memory": {
      "args": [
        "basic-memory",
        "mcp"
      ],
      "command": "uvx",
      "disabled": false
    },
    "dbhub-demo": {
      "args": [
        "-y",
        "@bytebase/dbhub",
        "--transport",
        "stdio",
        "--demo"
      ],
      "command": "npx",
      "disabled": true
    },
    "dbhub-jira-ai": {
      "args": [
        "-y",
        "@bytebase/dbhub",
        "--transport",
        "stdio",
        "--dsn",
        "sqlite:///Users/zacharylevinw/.local/share/nvim/jira-ai/jira_ai.db"
      ],
      "command": "npx",
      "disabled": true
    },
    "dbhub-plat": {
      "args": [
        "-y",
        "@bytebase/dbhub",
        "--transport",
        "stdio",
        "--dsn",
        "postgres://postgres:abc123@localhost:5432/postgres"
      ],
      "command": "npx",
      "disabled": true
    },
    "dbhub-revman": {
      "args": [
        "-y",
        "@bytebase/dbhub",
        "--transport",
        "stdio",
        "--dsn",
        "sqlite:///Users/zacharylevinw/.local/state/nvim/revman/revman.db"
      ],
      "command": "npx",
      "disabled": true
    },
    "figma": {
      "disabled": false,
      "type": "http",
      "url": "http://127.0.0.1:3845/mcp"
    },
    "github": {
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "command": "docker",
      "disabled": false,
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${env:GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "type": "stdio"
    },
    "kubernetes": {
      "args": [
        "mcp-server-kubernetes"
      ],
      "command": "npx",
      "disabled": false
    },
    "mcp-obsidian": {
      "args": [
        "mcp-obsidian"
      ],
      "command": "uvx",
      "disabled": true
    },
    "mui-mcp": {
      "args": [
        "-y",
        "@mui/mcp@latest"
      ],
      "command": "npx",
      "type": "stdio"
    },
    "nx-mcp": {
      "args": [
        "nx-mcp@latest"
      ],
      "command": "npx",
      "type": "stdio"
    },
    "tavily": {
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.tavily.com/mcp/?tavilyApiKey=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"
      ],
      "command": "npx"
    }
  }
}
