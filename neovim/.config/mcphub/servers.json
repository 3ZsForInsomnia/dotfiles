{
  "mcpServers": {
    "atlassian-mcp-server": {
      "type": "http",
      "url": "https://mcp.atlassian.com/v1/sse"
    },
    "basic-memory": {
      "args": [
        "basic-memory",
        "mcp"
      ],
      "command": "uvx"
    },
    "dbhub-demo": {
      "args": [
        "-y",
        "@bytebase/dbhub",
        "--transport",
        "stdio",
        "--demo"
      ],
      "command": "npx",
      "disabled": true
    },
    "dbhub-jira-ai": {
      "args": [
        "-y",
        "@bytebase/dbhub",
        "--transport",
        "stdio",
        "--dsn",
        "sqlite:///Users/zacharylevinw/.local/share/nvim/jira-ai/jira_ai.db"
      ],
      "command": "npx",
      "disabled": false
    },
    "dbhub-plat": {
      "args": [
        "-y",
        "@bytebase/dbhub",
        "--transport",
        "stdio"
      ],
      "command": "npx",
      "disabled": true
    },
    "dbhub-revman": {
      "args": [
        "-y",
        "@bytebase/dbhub",
        "--transport",
        "stdio",
        "--dsn",
        "sqlite:///Users/zacharylevinw/.local/state/nvim/revman/revman.db"
      ],
      "command": "npx",
      "disabled": false
    },
    "github": {
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "command": "docker",
      "disabled": false,
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${env:GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "type": "stdio"
    },
    "kubernetes": {
      "args": [
        "mcp-server-kubernetes"
      ],
      "command": "npx"
    },
    "mcp-obsidian": {
      "args": [
        "mcp-obsidian"
      ],
      "command": "uvx",
      "disabled": true
    },
    "nx-mcp": {
      "args": [
        "nx-mcp@latest"
      ],
      "command": "npx",
      "type": "stdio"
    }
  }
}
