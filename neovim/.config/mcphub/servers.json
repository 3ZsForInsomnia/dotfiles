{
  "mcpServers": {
    "basic-memory": {
      "args": ["basic-memory", "mcp"],
      "command": "uvx"
    },
    "dbhub-demo": {
      "args": ["-y", "@bytebase/dbhub", "--transport", "stdio", "--demo"],
      "command": "npx",
      "disabled": true
    },
    "dbhub-plat-qat": {
      "args": [
        "-y",
        "@bytebase/dbhub",
        "--transport",
        "stdio",
        "--dsn",
        "postgresql://adminTerraform@localhost:5555/postgres?sslmode=require"
      ],
      "command": "npx",
      "disabled": true
    },
    "dbhub-revman": {
      "args": [
        "-y",
        "@bytebase/dbhub",
        "--transport",
        "stdio",
        "--dsn",
        "sqlite:///Users/zacharylevinw/.local/state/nvim/revman/revman.db"
      ],
      "command": "npx",
      "disabled": false
    },
    "kubernetes": {
      "args": ["mcp-server-kubernetes"],
      "command": "npx"
    },
    "mcp-atlassian": {
      "args": [
        "run",
        "--rm",
        "-i",
        "--env-file",
        "~/src/zsh/.env-jira",
        "ghcr.io/sooperset/mcp-atlassian:latest"
      ],
      "command": "docker",
      "disabled": true
    },
    "mcp-obsidian": {
      "args": ["mcp-obsidian"],
      "command": "uvx",
      "disabled": true
    },
    "nx-mcp": {
      "args": ["nx-mcp@latest"],
      "command": "npx",
      "type": "stdio"
    }
  }
}
